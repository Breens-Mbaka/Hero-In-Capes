{{#partial "content"}}
    <h1>Your Squad</h1>
    {{#each myHeroes}}
        <div class="card">
            <div class="card-body">
                <div class="card-title">Hero</div>
                <div class="card-text"><strong>Name: </strong>{{name}}</div>
                <div class="card-footer"><strong>Squad : </strong>{{squadName}}</div>
            </div>
        </div>
    {{/each}}
{{/partial}}
{{> layout.hbs}}